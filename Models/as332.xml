<?xml version="1.0"?>

<PropertyList>
	<path>as332.ac</path>

	<offsets>
		<z-m>0.05</z-m>
		<y-m>0</y-m>
		<x-m>0</x-m>
		<pitch-deg>0.3</pitch-deg>
	</offsets>


	<!-- main rotor group -->
	
	<animation>
		<name>mainrotors</name>
		<object-name>blade01</object-name>
		<object-name>blade01a</object-name>
		<object-name>blade01b</object-name>
		<object-name>blade01c</object-name>
		<object-name>blade01d</object-name>
		<object-name>blade02</object-name>
		<object-name>blade02a</object-name>
		<object-name>blade02b</object-name>
		<object-name>blade02c</object-name>
		<object-name>blade02d</object-name>
		<object-name>blade03</object-name>
		<object-name>blade03a</object-name>
		<object-name>blade03b</object-name>
		<object-name>blade03c</object-name>
		<object-name>blade03d</object-name>
		<object-name>blade04</object-name>
		<object-name>blade04a</object-name>
		<object-name>blade04b</object-name>
		<object-name>blade04c</object-name>
		<object-name>blade04d</object-name>
	</animation>

<!-- tail rotor group -->
	
	<animation>
		<name>tailrotors</name>
		<object-name>tailblade01</object-name>
		<object-name>tailblade02</object-name>
		<object-name>tailblade03</object-name>
		<object-name>tailblade04</object-name>
		<object-name>tailblade05</object-name>
	</animation>


<!-- main rotor animation blurred -->


	<animation>
		<type>material</type>
		<object-name>blurred01</object-name>
		<object-name>blurred02</object-name>
		<object-name>blurred03</object-name>
		<object-name>blurred04</object-name>
		<transparency>
			<alpha-prop>rotors/main/rpm</alpha-prop>
			<factor>0.002</factor>
			<offset>-0.25</offset>
			<max>0.4</max>
		</transparency>
		<threshold>0.001</threshold>
	</animation>


	<animation>
		<type>select</type>
		<object-name>mainrotors</object-name>
		<condition>
			<less-than>
				<property>rotors/main/rpm</property>
				<value>175</value>
			</less-than>
		</condition>
	</animation>
	

	<animation>
		<type>spin</type>
		<object-name>blurred01</object-name>
		<object-name>blurred02</object-name>
		<object-name>blurred03</object-name>
		<object-name>blurred04</object-name>
		<property>rotors/main/rpm</property>
		<axis>
			<x>30</x>
			<y>0</y>
			<z>-360</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>3.212</z-m>
		</center>
	</animation>
	
	<animation>
		<type>noshadow</type>
		<object-name>blurred01</object-name>
		<object-name>blurred02</object-name>
		<object-name>blurred03</object-name>
		<object-name>blurred04</object-name>
	</animation>


<!-- main rotor blade grips -->

	<animation>
		<type>spin</type>
		<object-name>bladegrip01</object-name>
		<object-name>bladegrip02</object-name>
		<object-name>bladegrip03</object-name>
		<object-name>bladegrip04</object-name>
		<property>rotors/main/rpm</property>
