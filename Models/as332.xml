		<axis>
			<x>0</x>
			<y>-360</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>9.597</x-m>
			<y-m>0.479</y-m>
			<z-m>2.522</z-m>
		</center>
	</animation>
	
