<!-- Aerospatiale AS-332 L1, Superpuma, Yves Sablonier, Version 09-11-25 -->

<airplane mass="9920">

<approach speed="47" aoa="16">
  <control-setting axis="/controls/engines/engine[0]/throttle" value="0.3"/>
</approach>

<cruise speed="147" alt="0">
  <control-setting axis="/controls/engines/engine[0]/throttle" value="1.0"/>
</cruise>

<rotor name="main" x="-0.25" y="0" z="3.212" nx="0.087" ny="0" nz="1" fx="1" fy="0" fz="0" ccw="0" 
maxcollective="14.5" 
mincollective="-2.0" 
mincyclicele="-7.4" 
maxcyclicele="9.5"
mincyclicail="-5" 
maxcyclicail="6.5" 
diameter="15.6" 
numblades="4" 
weightperblade="115" 
twist="-9.5" 
chord="0.44" 
rpm="278" 
relbladecenter="0.5" 
dynamic="1.4" 
rellenflaphinge="0.15" 
delta3="0.3" 
delta=".25" 
pitch-a="10" 
pitch-b="15" 
flapmin="-15" 
flapmax="15" 
flap0="-7" 
flap0factor="0" 
phi0="45" 
notorque="0" 
dragfactor="0.30" 
translift-ve="20" 
translift-maxfactor="1.3" 
ground-effect-constant="0.7" 
taper="1" 
number-of-segments="8" 
number-of-parts="8" 
rel-len-where-incidence-is-measured="0.6" 
rel-len-blade-start="0.04" 
airfoil-lift-coefficient="6.3"
airfoil-drag-coefficient0="0.03" 
airfoil-drag-coefficient1="0.75" 
incidence-stall-zero-speed="15" 
incidence-stall-half-sonic-speed="14.5" 
lift-factor-stall="0.28" 
drag-factor-stall="2.0" 
stall-change-over="5.5" 
cyclic-factor="1" 
rotor-correction-factor="0.7"
>

    
  <control-input axis="/controls/flight/aileron-trim" control="CYCLICAIL"/>
  <control-input axis="/controls/flight/aileron" control="CYCLICAIL"
                 src0="-1.0" src1="1.0"
                 dst0="-1.0" dst1="1.0"/>
  <control-input axis="/controls/flight/elevator-trim" control="CYCLICELE"/>
  <control-input axis="/controls/flight/elevator" control="CYCLICELE"
                 src0="-1.0" src1="1.0"
                 dst0="-1.0" dst1="1.0"/>
  <control-input axis="/controls/engines/engine[0]/throttle" control="COLLECTIVE"
                 src0="0.0" src1="1.0"
                 dst0="1.0" dst1="-1.0"/>
</rotor>



<rotor name="tail" x="-9.597" y="-0.422" z="2.522" nx="0" ny="1" nz="0" fx="1" fy="0" fz="0" ccw="1"
	  maxcollective="15" mincollective="-5"
	  diameter="3.05" numblades="5" weightperblade="2.72" relbladecenter="0.5"
	  dynamic="1" rpm="1284" rellenflaphinge="0.0" delta3="0" translift="0" delta="0.3"
	  pitch-a="10"
	  pitch-b="15"
	  airfoil-lift-coefficient="6.4"
	  airfoil-drag-coefficient0="0.0045"
	  airfoil-drag-coefficient1="0.10"
	  notorque="0"
	  taper="1"
	  chord="0.22"
	  number-of-segments="5"
	  number-of-parts="4"
	  rel-len-blade-start="0.2"
	  incidence-stall-zero-speed="9"
	  incidence-stall-half-sonic-speed="18"
	  lift-factor-stall="0.18"
	  stall-change-over="5.5"
	  drag-factor-stall="8"
	  rotor-correction-factor="0.7"
>
  <control-input axis="/controls/flight/rudder-trim" control="COLLECTIVE"/>
  <control-input axis="/controls/flight/rudder" control="COLLECTIVE"
                 src0="-1.0" src1="1.0"
                 dst0="-1.0" dst1="1.0"/>
  <control-input axis="/controls/engines/engine[0]/throttle" control="COLLECTIVE"
                 src0="0.1" src1="1.0"
                 dst0=".0007" dst1="-.00055"/>
</rotor>


<!-- max. Take-off Power 1357 kW per engine -->
<rotorgear
  max-power-engine="2714"
  max-power-rotor-brake="100"
  rotorgear-friction="1.4"
  engine-prop-factor="0.005"
  engine-accel-limit="4"
  yasimdragfactor="10"
  yasimliftfactor="160"
>
  <control-input axis="/controls/engines/engine[0]/magnetos" control="ROTORGEARENGINEON"/>
  <control-input axis="/controls/rotor/brake" control="ROTORBRAKE"
                 src0="0.0" src1="1.0"
                 dst0="0.0" dst1="1.0"/>
  <control-input axis="/controls/rotor/reltarget" control="ROTORRELTARGET"
                 src0="0.0" src1="2.0"
                 dst0="0.0" dst1="2.0"/>
  <control-input axis="/controls/rotor/maxreltorque" control="ROTORENGINEMAXRELTORQUE"
                 src0="0.0" src1="2.0"
                 dst0="0.0" dst1="2.0"/>
</rotorgear>


<cockpit x="-3.251" y="0.459" z="-2.262"/>


<!-- body -->
<fuselage ax="-4.761" ay="0" az="0.18" bx="4.758" by="0" bz="0.931"
          width="2.14" taper="0.5" midpoint="1.0" idrag="0"/>

<!-- tail -->
<fuselage ax="4.758" ay="0" az="0.931" bx="9.397" by="0" bz="1.089"
          width="0.8" taper="0.5" midpoint="1.0" idrag="0"/>
          
<!-- vstab --> 
<vstab x="8.398" y="0" z="0.754" taper="0.9" length="1.8" chord="1.0"
sweep="47">
</vstab>

<!-- front gear -->
<gear x="-3.261" y="0" z="-1.237" compression="0.5" sfric="0.8" dfric="0.8" retract-time="4"
    spring="15" damp="1.5" initial-load="0">
    <control-input axis="/controls/flight/rudder" control="STEER" square="true"
                 src0="-1.0" src1="1.0"
                 dst0="-1.0" dst1="1.0"/>
    <control-output control="STEER" prop="controls/gear/steering"/>
    <control-input axis="/controls/gear/gear-down" control="EXTEND"/>
    <control-speed control="EXTEND" transition-time="4"/>
    <control-output control="EXTEND" prop="/gear/gear[0]/position-norm"/>
  </gear>
  
<!-- left gear -->
  <gear x="2.039" y="-1.49" z="-1.216" compression="0.7" sfric="0.8" dfric="0.8" retract-time="4"
    spring="15" damp="1.5" initial-load="0">
    <control-input axis="/controls/gear/brake-left" control="BRAKE"/>
    <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
    <control-input axis="/controls/gear/gear-down" control="EXTEND"/>
    <control-speed control="EXTEND" transition-time="4"/>
    <control-output control="EXTEND" prop="/gear/gear[1]/position-norm"/>
  </gear>
  
<!-- right gear -->
  <gear x="2.039" y="1.49" z="-1.216" compression="0.7" sfric="0.8" dfric="0.8" retract-time="4"
    spring="15" damp="1.5" initial-load="0">
    <control-input axis="/controls/gear/brake-right" control="BRAKE"/>
    <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
    <control-input axis="/controls/gear/gear-down" control="EXTEND"/>
    <control-speed control="EXTEND" transition-time="4"/>
    <control-output control="EXTEND" prop="/gear/gear[2]/position-norm"/>
  </gear>


<!-- standard tanks -->
<tank x="0" y="1.4" z="-0.2" capacity="1758" jet="true"/>
<tank x="0" y="-1.4" z="-0.2" capacity="1758" jet="true"/>

<!-- ballast -->
<!-- adjust the cg-->
    <ballast x="-3" y="0" z="0.1" mass="1000"/>

<!-- adjust inertia tensor-->
    <ballast x="0" y="-0.5" z="0.1" mass="2000"/>
    <ballast x="0" y="-0.2" z="0.1" mass="2000"/>
    <ballast x="0" y="0" z="0.1" mass="500"/>
    <ballast x="0" y="0" z="0.1" mass="500"/>

    <!-- crew -->
<weight x="-3.251" y="0.459" z="-1.262" mass-prop="/sim/weight[0]/weight-lb"/>	<!-- pilot -->
<weight x="-3.251" y="-0.459" z="-1.262" mass-prop="/sim/weight[1]/weight-lb"/>	<!-- co-pilot -->

<thruster x="-2.55" y="0.0" z="1.55" vx="1" vy="0" vz="0" thrust="4000">
  <control-input axis="/rotors/main/vibration/longitudinal" src0="-1" src1="1" dst0="-1" dst1="1" control="THROTTLE"/>
</thruster>

<thruster x="-2.55" y="0.0" z="1.55" vx="0" vy="1" vz="0" thrust="4000">
  <control-input axis="/rotors/main/vibration/lateral" src0="-1" src1="1" dst0="-1" dst1="1" control="THROTTLE"/>
</thruster>


</airplane>
